{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/main.tsx"],
  "project": ["src/**/*.{ts,tsx}", "!src/**/*.test.{ts,tsx}", "!src/**/*.spec.{ts,tsx}"],
  "rules": {
    "files": "error",
    "dependencies": "error",
    "unlisted": "error",
    "exports": "error",
    "nsExports": "error",
    "classMembers": "off"
  }
}
